{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist", 
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "api/voice-agent.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/voice-agent",
      "dest": "/api/voice-agent.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
